{
    "actions": [
        {
            "command": "deploy_linaro_image",
            "parameters": {
                "image": "http://images.validation.linaro.org/kvm/large-sid.img.gz"
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "http://github.com/iwishguo/LAVA-script.git",
                        "revision": "master",
                        "testdef": "linaro-edk2-build-fetch-source.yaml",
                        "parameters": {
                            "GCC_URL": "http://releases.linaro.org/13.04/components/toolchain/binaries/gcc-linaro-aarch64-linux-gnu-4.7-2013.04-20130415_linux.tar.xz",
														"GCC_REV": "4.7" 
                        },
                        "testdef": "linaro-edk2-build-fetch-gcc-aarch64.yaml",
                        "testdef": "linaro-edk2-build-aarch64-all.yaml",
                        "testdef": "linaro-edk2-build-remove-gcc-aarch64.yaml",
                         "parameters": {
                            "GCC_URL": "http://releases.linaro.org/14.04/components/toolchain/binaries/gcc-linaro-aarch64-linux-gnu-4.8-2014.04_linux.tar.xz",
														"GCC_REV": "4.8" 
                        },
                        "testdef": "linaro-edk2-build-fetch-gcc-aarch64.yaml",
                        "testdef": "linaro-edk2-build-aarch64-all.yaml",
                        "testdef": "linaro-edk2-build-remove-gcc-aarch64.yaml",
                          "parameters": {
                            "GCC_URL": "http://releases.linaro.org/14.11/components/toolchain/binaries/aarch64-linux-gnu/gcc-linaro-4.9-2014.11-x86_64_aarch64-linux-gnu.tar.xz",
														"GCC_REV": "4.9" 
                        },
                        "testdef": "linaro-edk2-build-fetch-gcc-aarch64.yaml",
                        "testdef": "linaro-edk2-build-aarch64-all.yaml",
                        "testdef": "linaro-edk2-build-remove-gcc-aarch64.yaml"
                   }
                ],
                "timeout": 18000
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "http://validation.linaro.org/RPC2/",
                "stream": "/anonymous/heyi-guo/"
            }
        }
    ],
    "device_type": "kvm",
    "job_name": "pure-linaro-edk2-build-test",
    "timeout": 1800
}


{
    "actions": [
        {
            "command": "deploy_linaro_image",
            "parameters": {
                "image": "http://images.validation.linaro.org/kvm/large-sid.img.gz"
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "http://github.com/iwishguo/LAVA-script.git",
                        "parameters": {
                            "BUILD_OPTS": "",
                            "TARGET": ""
                        },
                        "revision": "master",
                        "testdef": "build-linaro-edk2.yaml"
                    }
                ],
                "timeout": 18000
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "http://validation.linaro.org/RPC2/",
                "stream": "/anonymous/heyi-guo/"
            }
        }
    ],
    "device_type": "kvm",
    "job_name": "pure-linaro-edk2-build-test",
    "timeout": 1800
}

